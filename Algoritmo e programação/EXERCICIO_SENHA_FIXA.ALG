Algoritmo "exercicio_senha_fixa"

Var
senha, x : inteiro

Inicio
     senha <- 456789
     escreva("Senha: ")
     leia(x)
     
     enquanto x <> senha faca
     escreva("Senha Invalida! Tente novamente: ")
     leia(x)
     fimenquanto
     
     escreval("Acesso permitido!")
     


Fimalgoritmo