Algoritmo "estrutura condicional encadeado"

Var
nota1, nota2, total : inteiro

Inicio
     escreva("Digite a primeira nota: ")
     leia(nota1)
     escreva("Digite a segunda nota: ")
     leia(nota2)
     total <- nota1 + nota2
     escreval("Nota final: ", total:2:2)
     se total < 60.0 entao
       escreval("Reprovado")
     fimse

Fimalgoritmo
